def make_it_laugh(string):
  new_string = string.replace("a", "haha")
  new_string = new_string.replace("e", "haha")
  new_string = new_string.replace("i", "haha")
  new_string = new_string.replace("o", "haha")
  new_string = new_string.replace("u", "haha")
  print(new_string)

make_it_laugh("My essay on the cold war has to be finished tomorrow. I'm really Russian to finish it")
